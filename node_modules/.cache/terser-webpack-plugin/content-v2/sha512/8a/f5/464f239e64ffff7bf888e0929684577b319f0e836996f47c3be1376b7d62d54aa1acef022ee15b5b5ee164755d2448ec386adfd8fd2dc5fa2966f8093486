ewItem = {\n                slug: product.slug,\n                color: color,\n                size: size,\n                price: product.price,\n                quantity: quantity\n            }\n            if (dispatch(addItem(newItem))) {\n                alert('Added to cart successfully')\n            } else {\n                alert('Failed to add to cart')\n            }\n        }\n    }\n\n    const goToCart = () => {\n        if (check()) {\n            let newItem = {\n                slug: product.slug,\n                color: color,\n                size: size,\n                price: product.price,\n                quantity: quantity\n            }\n            if (dispatch(addItem(newItem))) {\n                dispatch(remove())\n                props.history.push('/cart')\n            } else {\n                alert('Failed to proceed to cart')\n            }\n        }\n    }\n\n    return (\n        <div className=\"product\">\n            <div className=\"product__images\">\n                <div className=\"product__images__list\">\n                    <div className=\"product__images__list__item\" onClick={() => setPreviewImg(product.image01)}>\n                        <img src={product.image01} alt=\"\" />\n                    </div>\n                    <div className=\"product__images__list__item\" onClick={() => setPreviewImg(product.image02)}>\n                        <img src={product.image02} alt=\"\" />\n                    </div>\n                </div>\n                <div className=\"product__images__main\">\n                    <img src={previewImg} alt=\"\" />\n                </div>\n                <div className={`product-description ${descriptionExpand ? 'expand' : ''}`}>\n                    <div className=\"product-description__title\">\n                        Product Details\n                    </div>\n                    <div className=\"product-description__content\" dangerouslySetInnerHTML={{__html: product.description}}></div>\n                    <div className=\"product-description__toggle\">\n                        <Button size=\"sm\" onClick={() => setDescriptionExpand(!descriptionExpand)}>\n                            {\n                                descriptionExpand ? 'Show Less' : 'Read More'\n                            }\n                        </Button>\n                    </div>\n                </div>\n            </div>\n            <div className=\"product__info\">\n                <h1 className=\"product__info__title\">{product.title}</h1>\n                <div className=\"product__info__item\">\n                    <span className=\"product__info__item__price\">\n                        {numberWithCommas(product.price)}\n                    </span>\n                </div>\n                <div className=\"product__info__item\">\n                    <div className=\"product__info__item__title\">\n                        Color\n                    </div>\n                    <div className=\"product__info__item__list\">\n                        {\n                            product.colors.map((item, index) => (\n                                <div key={index} className={`product__info__item__list__item ${color === item ? 'active' : ''}`} onClick={() => setColor(item)}>\n                                    <div className={`circle bg-${item}`}></div>\n                                </div>\n                            ))\n                        }\n                    </div>\n                </div>\n                <div className=\"product__info__item\">\n                    <div className=\"product__info__item__title\">\n                        Size\n                    </div>\n                    <div className=\"product__info__item__list\">\n                        {\n                            product.size.map((item, index) => (\n                                <div key={index} className={`product__info__item__list__item ${size === item ? 'active' : ''}`